{
  "private": true,
  "name": "webpack-cfg-example",
  "version": "1.0.0",
  "description": "A webpack-cfg sample",
  "author": "Adrian C. Miranda",
  "homepage": "https://npmjs.com/package/webpack-cfg",
  "module": "public/index.js",
  "scripts": {
    "preinstall": "npm install ../",
    "upgrade": "ncu -a --loglevel verbose --packageFile package.json && npm i",
    "bench": "node @bin/benchmark",
    "test": "echo \"Error: no test specified\" && exit 0",
    "start": "node @bin/www",
    "dev": "cross-env NODE_ENV=development npm start -- --run client-dev,server-dev --debug 1 --log 1 --notify 1",
    "server": "npm run dev",
    "client": "webpack-dev-server --config @bin/webpack --env.run client-dev --env.lifecycle=dev",
    "client:async": "webpack-dev-server --config @bin/webpack/async --env.run client-dev --env.lifecycle=dev",
    "build:client": "webpack --config @bin/webpack --env.run client-build",
    "build:server": "webpack --config @bin/webpack --env.run server-build",
    "build": "webpack --config @bin/webpack --env.run client-build,server-build",
    "stats": "webpack --config @bin/webpack --profile --json > stats.json",
    "flow": "flow"
  },
  "optionalDependencies": {
    "compression-webpack-plugin": "1.1.11",
